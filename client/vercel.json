{
	"rewrites": [
		{
			"source": "/static/:path*",
			"destination": "/static/:path*"
		},
		{
			"source": "/assets/:path*",
			"destination": "/assets/:path*"
		},
		{
			"source": "/favicon.ico",
			"destination": "/favicon.ico"
		},
		{
			"source": "/:path*",
			"destination": "/index.html"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "s-maxage=0"
				}
			]
		},
		{
			"source": "/assets/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		}
	]
}